{
  "productName": "Tauri-Docs",
  "version": "2.0.7",
  "identifier": "io.github.roseblume.tdocs",
  "build": {
    "beforeDevCommand": "pnpm run dev",
    "devUrl": "http://localhost:4321",
    "beforeBuildCommand": "pnpm run build",
    "frontendDist": "../dist"
  },
  "app": {"windows": [
      {
        "title": "Tauri Docs",
        "label": "main",
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["deb","rpm", "nsis"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}